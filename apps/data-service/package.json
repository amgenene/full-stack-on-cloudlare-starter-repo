{
	"name": "data-service",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"@repo/data-ops": "workspace:*",
		"deploy": "wrangler deploy --env $CF_ENVIRONMENT",
		"build": "rm ./worker-configuration.d.ts && npm run cf-typegen",
		"dev": "wrangler dev --x-remote-bindings",
		"start": "wrangler dev",
		"test": "vitest",
		"migrate:init": "wrangler d1 export DB --output=dump.sql --env stage --remote",
		"cf-typegen": "wrangler types --env-interface BaseEnv --env $CF_ENVIRONMENT"
	},
	"devDependencies": {
		"@cloudflare/puppeteer": "^1.0.3",
		"@cloudflare/vitest-pool-workers": "^0.8.19",
		"@types/node": "^22.15.19",
		"tsc": "^2.0.4",
		"typescript": "^5.5.2",
		"vitest": "~3.2.0",
		"wrangler": "^4.33.0"
	},
	"dependencies": {
		"@repo/data-ops": "workspace:*",
		"ai": "^4.3.16",
		"hono": "^4.8.3",
		"moment": "^2.30.1",
		"workers-ai-provider": "^0.7.1",
		"zod": "^3.25.67"
	}
}
